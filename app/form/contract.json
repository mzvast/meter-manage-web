{
  "title": "试验委托协议",
  "fields": [
    {
      "type":"custom",
      "className":"row",
      "templateOptions": {
        "label": "客户信息"
      }
    },
    {
      "className": "row",
      "fieldGroup": [
        {
          "className": "col-xs-6",
          "key": "requester",
          "type": "input",
          "templateOptions": {
            "label": "委托单位",
            "placeholder": "XXXXXXXX有限公司"
          }
        },
        {
          "className": "col-xs-6",
          "key": "generatingUnit",
          "type": "input",
          "templateOptions": {
            "label": "生产单位",
            "placeholder": "XXXXXXXX有限公司"
          }
        }
      ]
    },
    {
      "className": "row",
      "fieldGroup": [
        {
          "className": "col-xs-8",
          "key": "address",
          "type": "input",
          "templateOptions": {
            "label": "地址",
            "placeholder": "XXX省XXX市XXXXXXXXXXXX"
          }
        },
        {
          "className": "col-xs-4",
          "key": "zipcode",
          "type": "input",
          "templateOptions": {
            "label": "邮政编码",
            "placeholder": "XXXXXXXX"
          }
        }
      ]
    },
    {
      "className": "row",
      "fieldGroup": [
        {
          "className": "col-xs-4",
          "key": "contact",
          "type": "input",
          "templateOptions": {
            "label": "联系人",
            "placeholder": "XXX"
          }
        },
        {
          "className": "col-xs-4",
          "key": "fax",
          "type": "input",
          "templateOptions": {
            "label": "传真",
            "placeholder": "XXXXXXXX"
          }
        },
        {
          "className": "col-xs-4",
          "key": "contactTel",
          "type": "input",
          "templateOptions": {
            "label": "电话",
            "placeholder": "XXX-XXXXXXXX "
          }
        }
      ]
    },
    {
      "className":"row",
      "type": "repeatSection",
      "key": "products",
      "templateOptions": {
        "btnText":"添加产品",
        "label":"样品信息",
        "fields": [
          {
            "className":"col-xs-2",
            "key":"name",
            "type":"input",
            "templateOptions": {
              "label": "样品名称"
            }
          },
          {
            "className":"col-xs-2",
            "key":"model",
            "type":"input",
            "templateOptions": {
              "label": "型号规格"
            }
          },
          {
            "className":"col-xs-2",
            "key":"code",
            "type":"input",
            "templateOptions": {
              "label": "出厂编号"
            }
          },
          {
            "className":"col-xs-2",
            "key":"produceUnit",
            "type":"input",
            "templateOptions": {
              "label": "制造单位"
            }
          },
          {
            "className":"col-xs-2",
            "key":"volume",
            "defaultValue":"",
            "type":"input",
            "templateOptions": {
              "label": "数量",
              "type": "number",
              "max": 99999,
              "min": 0
            }
          },
          {
            "className":"col-xs-2",
            "key":"PS",
            "type":"input",
            "templateOptions": {
              "label": "备注"
            }
          }
        ]
      }
    },
    {
      "type":"custom",
      "className":"row",
      "templateOptions": {
        "label": "委托要求"
      }
    },
    {
      "className": "row",
      "fieldGroup": [
        {
          "className": "col-xs-6",
          "key": "testType",
          "defaultValue": "委托试验",
          "type": "select",
          "templateOptions": {
            "label": "试验类型",
            "valueProp": "name",
            "options": [
              {
                "name": "委托试验"
              },
              {
                "name": "型式试验"
              },
              {
                "name": "抽样试验"
              },
              {
                "name": "其他试验"
              }
            ]
          }
        },
        {
          "className": "col-xs-6",
          "key": "standard",
          "defaultValue": "行业标准",
          "type": "select",
          "templateOptions": {
            "label": "依据标准",
            "valueProp": "name",
            "options": [
              {
                "name": "国家标准"
              },
              {
                "name": "检定规程"
              },
              {
                "name": "型式评价大纲"
              },
              {
                "name": "行业标准"
              },
              {
                "name": "企业标准"
              },
              {
                "name": "国际标准"
              },
              {
                "name": "其他"
              }
            ]
          }
        }

      ]
    },
    {
      "className": "row",
      "key": "productInfo",
      "fieldGroup": [
        {
          "className": "col-xs-6",
          "key": "throwMethod",
          "defaultValue": "自取",
          "type": "select",
          "templateOptions": {
            "label": "处置方式",
            "valueProp": "name",
            "options": [
              {
                "name": "自取"
              },
              {
                "name": "自愿放弃"
              },
              {
                "name": "其他"
              }
            ]
          }
        },
        {
          "className": "col-xs-6",
          "key": "sendMethod",
          "defaultValue": "送样",
          "type": "select",
          "templateOptions": {
            "label": "收样方式",
            "valueProp": "name",
            "options": [
              {
                "name": "送样"
              },
              {
                "name": "抽检"
              },
              {
                "name": "现场"
              },
              {
                "name": "内部周检"
              }
            ]
          }
        }
      ]
    },
    {
      "type":"custom",
      "className":"row",
      "templateOptions": {
        "label": "报告信息",
        "hr":true
      }
    },
    {
      "className": "row",
      "key": "reportInfo",
      "fieldGroup": [
        {
          "className": "col-xs-4",
          "key": "type",
          "defaultValue": "检验报告",
          "type": "select",
          "templateOptions": {
            "label": "报告类型",
            "valueProp": "name",
            "options": [
              {
                "name": "检验证书"
              },
              {
                "name": "校准证书"
              },
              {
                "name": "测试证书"
              },
              {
                "name": "检验报告"
              },
              {
                "name": "检测报告"
              },
              {
                "name": "测试报告"
              },
              {
                "name": "型式评价报告"
              }
            ]
          }
        },
        {
          "className": "col-xs-4",
          "key": "style",
          "defaultValue": "中文版",
          "type": "select",
          "templateOptions": {
            "label": "报告版式",
            "valueProp": "name",
            "options": [
              {
                "name": "中文版"
              },
              {
                "name": "英文版"
              },
              {
                "name": "其他"
              }
            ]
          }
        },
        {
          "className": "col-xs-4",
          "key": "sendMethod",
          "defaultValue": "自取",
          "type": "select",
          "templateOptions": {
            "label": "报告提取方式",
            "valueProp": "name",
            "options": [
              {
                "name": "自取"
              },
              {
                "name": "邮寄"
              }
            ]
          }
        }
      ]
    }

  ]
}

